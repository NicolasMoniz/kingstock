<body>
<div id="js-cookie-box" class="cookie-box cookie-box--hide">
    En poursuivant votre navigation sur ce site, vous acceptez l'utilisation de cookies üç™ ! <span id="js-cookie-button" class="cookie-button">√áa marche !</span>
</div>

<section class="pre-header">
    <div class="menu-top-right">
        <span>{{ lang.Roi_des_bonnes_affaires }}</span>
        <div class="flex align-items-center">
            <div id="menuUser" class="menu-user">
                {% if session.co == true %}
                    {% if  session.admin==1 %}
                        <a href="{{ base_url("admin/index/index") }}"><div class="menu-user-subscribe">Admin</div></a>
                        <span class="pipe">|</span>
                    {% endif %}
                    <a href="{{ base_url("Account/index") }}"><div class="menu-user-subscribe">{{ lang.Panier }}</div></a>
                    <span class="pipe">|</span>
                    <a href="{{ base_url("Account/index") }}"><div class="menu-user-subscribe">{{ lang.Moncompte }}</div></a>
                    <span class="pipe">|</span>
                    <a href="{{ base_url("Account/logout") }}"><div class="menu-user-connect">{{ lang.Deconnexion }}</div></a>
                {% else %}
                    <a href="{{ base_url("Account/register") }}"><div class="menu-user-subscribe">{{ lang.S_inscrire }}</div></a>
                    <span class="pipe">|</span>
                    <a href="{{ base_url("Account/login") }}"><div class="menu-user-connect">{{ lang.Se_connecter }}</div></a>
                {% endif %}
            </div>
            <div class="lang-selection">
                {% set i=0 %}
                {% for langs in lang_array %}
                    {% if langs.code == session.lang %}
                        <a href="{{ base_url("home/language?lang="~langs.code) }}" class="lang active">{{ langs.code }}</a> |
                    {% elseif lang_array|length == i+1 %}
                        <a href="{{ base_url("home/language?lang="~langs.code) }}" class="lang">{{ langs.code }}</a>

                    {% else %}
                        <a href="{{ base_url("home/language?lang="~langs.code) }}" class="lang">{{ langs.code }}</a> |
                    {% endif %}
                    {% set i = i+1 %}
                {% endfor %}
            </div>
        </div>
    </div>
</section>
